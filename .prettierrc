{
    "semi": true,
    "trailingComma": "all",
    "singleQuote": true,
    "printWidth": 120,
    "tabWidth": 2,
    "arrowParens": "always",
    "endOfLine": "auto",
    "overrides": [
        {
            "files": "*.ts",
            "options": {
                "parser": "typescript",
                "useTabs": true,
                "jsxSingleQuote": true
            }
        }
    ]
}
